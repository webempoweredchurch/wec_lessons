#tabPane {
	float: left;
	width: 100%;
	margin-left: 5px;
}

#tabPane .tabcontent {
	display: none;
	margin: 0 20px 0 0;
}
#tabPane .tabcontent.active {
	display: block;
	xwidth: 90%;
	border: 2px solid #94946C;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
}
#tabMenu ul li.active a span {
	color: #ec4;
	font-weight: bold;
}
.hide {
	display: none !important;
}
.tabArrow {
	display: inline;
	margin: 4px;
	margin-right: 10px;
}

#tabMenu {
	width: 100%;
	height: 28px;
	font-size: 93%;
	line-height: 1.3em;
}
#tabMenu ul {
	margin: 0;
	padding: 0px 10px 0 50px;
	list-style: none;
}
#tabMenu li {
	display: inline !important;
	margin: 0;
	padding: 0;
}
#tabMenu a {
	float: left;
	display: inline;
	background: url("images/tableftK.gif") no-repeat left top;
	margin: 0;
	padding: 0 0 0 4px;
	text-decoration: none !important;
}
#tabMenu a span {
	display: block;
	background: url("images/tabrightK.gif") no-repeat right top;
	padding: 5px 15px 4px 6px;
	color: #FFF;
}
#tabMenu a:hover span {
	color: #FFF;
	background-position: 100% -42px;
}
#tabMenu a:hover {
	background-position: 0% -42px;
}

